<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.4.6" version="2.0"
  xmlns="http://pear.php.net/dtd/package-2.0"
  xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd">
  <name>drDoctrineCompilePlugin</name>
  <channel>pear.symfony-project.com</channel>
  <summary>Provides a doctrine:compile task</summary>
  <description>This plugin provides a doctrine:compile-core task which compiles many used Doctrine classes and loads the resulting file, instead of the Doctrine files in the vendor directory.</description>
  <lead>
    <name>Matthias Noback</name>
    <user>matthias_at_driebit</user>
    <email>matthias@driebit.nl</email>
    <active>yes</active>
  </lead>
  <date>2011-03-17</date>
  <time>13:00:00</time>
  <version>
    <release>1.0.0</release>
    <api>1.0.0</api>
  </version>
  <stability>
    <release>stable</release>
    <api>stable</api>
  </stability>
  <license uri="http://www.symfony-project.org/license">MIT license</license>
  <notes>-</notes>
  <contents>
    <dir name="/">
      <file role="data" name="README" />
      <file role="data" name="LICENSE" />
      <dir name="config">
        <file role="data" name="drDoctrineCompilePluginConfiguration.class.php" />
      </dir>
      <dir name="lib">
        <dir name="doctrine">
          <file role="data" name="drDoctrineCompiler.class.php" />
          <file role="data" name="Doctrine.php" />
        </dir>
        <dir name="exception">
          <file role="data" name="drDoctrineCompilerException.class.php" />
        </dir>
        <dir name="task">
          <file role="data" name="drDoctrineCompileTask.class.php" />
        </dir>
      </dir>
    </dir>
  </contents>
  <dependencies>
    <required>
      <php>
        <min>5.1.0</min>
      </php>
      <pearinstaller>
        <min>1.4.1</min>
      </pearinstaller>
      <package>
        <name>symfony</name>
        <channel>pear.symfony-project.com</channel>
        <min>1.4.0</min>
        <max>1.5.0</max>
        <exclude>1.5.0</exclude>
      </package>
      <package>
        <name>sfDoctrinePlugin</name>
        <channel>pear.symfony-project.com</channel>
        <min>1.0.0</min>
        <max>2.0.0</max>
        <exclude>2.0.0</exclude>
      </package>
    </required>
  </dependencies>
  <phprelease />
  <changelog>
    <release>
      <version>
        <release>1.0.0</release>
        <api>1.0.0</api>
      </version>
      <stability>
        <release>stable</release>
        <api>stable</api>
      </stability>
      <date>2011-03-17</date>
      <license uri="http://www.symfony-project.org/license">MIT license</license>
      <notes>First public release</notes>
    </release>
  </changelog>
</package>